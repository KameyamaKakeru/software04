<%= form_with url: '/top/login', method: :post, local: true do |f| %>
    <%=f.label :uid,'ユーザID' %>
    <%=f.text_field :uid %>
    <%=f.label :pass,'パスワード' %>
    <%=f.password_field :pass %>
    <%=f.submit 'ログイン' %>
<% end %> 

<%= link_to "ユーザー登録" ,new_user_path %>

<%  link_to "ユーザー一覧" ,users_path %>